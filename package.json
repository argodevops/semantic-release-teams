{
  "name": "@argodevops/semantic-release-teams",
  "version": "1.0.26",
  "description": "plugin for semantic release to notify multiple teams channels of a release",
  "source": "src/index.js",
  "main": "dist/index.js",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "jest",
    "lint": "eslint \"./src/**\" --fix",
    "pretty": "prettier --write \"src/**/*.{js*,ts}\"",
    "prepare": "is-ci || husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/argodevops/semantic-release-teams.git"
  },
  "keywords": [
    "semantic-release"
  ],
  "author": "ryan.b@argodevops.co.uk",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/argodevops/semantic-release-teams/issues"
  },
  "homepage": "https://github.com/argodevops/semantic-release-teams#readme",
  "files": [
    "dist",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@argodevops/eslint-config": "^1.4.0",
    "@argodevops/semantic-release-teams": "1.0.16",
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@parcel/config-default": "^2.15.4",
    "@parcel/transformer-js": "^2.15.4",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/npm": "^12.0.2",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/jest": "^30.0.0",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.31.0",
    "husky": "^9.1.7",
    "is-ci": "^3.0.1",
    "jest": "^30.0.4",
    "parcel": "^2.15.4",
    "semantic-release": "^24.2.7",
    "semantic-release-npm-deprecate": "^1.0.34"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@semantic-release/error": "^4.0.0",
    "@semantic-release/git": "^10.0.1",
    "axios": "^1.10.0"
  },
  "pnpm": {
    "overrides": {
      "json5@<2.2.2": ">=2.2.2",
      "debug@<3.1.0": ">=3.1.0",
      "semver@<7.5.2": ">=7.5.2"
    }
  }
}
